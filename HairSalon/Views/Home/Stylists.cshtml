@{
  Layout = "_Layout";
}

<div class="container">
        <h3>Stylists: </h3>
        @if (@Model.Count == 0)
        {
          <h5>No Stylists have been added yet!</h5>
        }
        <br>
        <hr>
        <ol>
            @foreach (var stylist in Model)
            {
              <li><a href='/stylists/@stylist.GetId()'>@stylist.GetName()</a></li>
            }
        </ol>
        <hr>
        <br>
        <hr>
        <p><a href='/'>Home</a></p>
        <hr>
    </div>
